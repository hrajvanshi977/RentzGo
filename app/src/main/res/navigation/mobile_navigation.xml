<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_myads">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.india.rentzgo.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />


    <fragment
        android:id="@+id/navigation_myads"
        android:name="com.india.rentzgo.ui.myads.MyAdsFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_myads" />

    <fragment
        android:id="@+id/navigation_doPost"
        android:name="com.india.rentzgo.ui.doPost.PostFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.india.rentzgo.ui.notifications.NotificationFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.india.rentzgo.ui.profile.ProfileFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

</navigation>